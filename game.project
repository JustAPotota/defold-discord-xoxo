[project]
title = XOXO
dependencies = https://github.com/britzl/gooey/archive/9.1.1.zip,https://github.com/britzl/monarch/archive/3.2.2.zip

[bootstrap]
main_collection = /hotseat/hotseat.collectionc

[input]
game_binding = /xoxo/input/game.input_bindingc

[display]
width = 640
height = 1136

[script]
shared_state = 1

[library]
include_dirs = xoxo

[collection_proxy]
max_count = 9

[html5]
scale_mode = stretch
show_fullscreen_button = 0
show_made_with_defold = 0

[android]
package = com.defold.game.xoxo

[osx]
bundle_identifier = com.defold.game.xoxo

[ios]
bundle_identifier = com.defold.game.xoxo

